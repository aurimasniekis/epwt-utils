{
  "name": "epwt/utils",
  "type": "symfony-bundle",
  "description": "EPWT Common used utils",
  "keywords": ["epwt", "utils"],
  "homepage": "http://github.com/gcds",
  "license": "MIT",
  "authors": [
    {
      "name": "Aurimas Niekis",
      "email": "aurimas.niekis@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6"
  },
  "suggest": {
    "ext-igbinary": "Faster and smaller serializer for SerializerTrait"
  },
  "autoload": {
    "psr-4": { "EPWT\\UtilsBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}
